<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="https://www.w3.org/2005/Atom">

 <title>Gurjot Sidhu</title>
 <link href="https://gsidhu.github.io/atom.xml" rel="self"/>
 <link href="https://gsidhu.github.io/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>https://gsidhu.github.io/</id>
 <author>
   <name>Gurjot Sidhu</name>
   <email>gsidhu22 at gmail dot com</email>
 </author>

 {% for post in site.posts %}
 <item>
   <title>{{ post.title }}</title>
   <link href="https://gsidhu.github.io{{ post.url }}"/>
   <pubdate>{{ post.date | date_to_xmlschema }}</pubdate>
   <dc:creator> Gurjot Sidhu </dc:creator>
   <id>gsidhu.github.io{{ post.id }}</id>
   <description type="html">{{ post.excerpt | xml_escape }}</description>
 </item>
 {% endfor %}

</feed>
